<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="#746D69"
                android:endColor="#746D69"
                android:centerColor="#FFF"
                android:angle="90"/>
        </shape>
    </item>
    <item
        android:top="1.5dp"
        android:bottom="1.5dp"
        android:left="1.5dp"
        android:right="1.5dp">
        <shape android:shape="oval">
            <stroke android:width="3dp" android:color="#000022"/>
        </shape>
    </item>
</layer-list>